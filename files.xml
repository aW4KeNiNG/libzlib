		<file name="${NATIVE_TOOLKIT_PATH}/zlib/gzlib.c" />
		<file name="${NATIVE_TOOLKIT_PATH}/zlib/inffast.c" />
		<file name="${NATIVE_TOOLKIT_PATH}/zlib/inftrees.c" />
